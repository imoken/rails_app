<table>
  <tr>
    <th>ISBN</th><th>署名</th><th>価格</th>
    <th>出版社</th><th>創刊日</th><th>CD</th>
  </tr>

<% @books.each do |book| %>
<tr>
  <td><%= book.isbn %></td>
  <td><%= book.title %></td>
  <td><%= book.price %></td>
  <td><%= book.publish %></td>
  <td><%= book.published %></td>
  <td><%= book.cd %></td>
</tr>
<% end %>
</table>
